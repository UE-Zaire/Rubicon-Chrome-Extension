{
  "manifest_version": 2,

  "name": "Rubicon",
  "description": "Keep track of your history/tabs!",
  "version": "0.0.1",
  "key": "",

  "oauth2": {
    "client_id": "102465963719-qv92vab4m0kjec7k1hs63jvffdu0b8mj.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/userinfo.profile"]
  },

  "icons": {
    "16": "assets/logo.png",
    "48": "assets/logo.png",
    "128": "assets/logo.png"
  },

  "browser_action": {
    "default_title": "menu"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_globs": ["http://localhost:3000/*", "https://www.facebook.com/*", "https://mail.google.com/*", "https://twitter.com/*", "https://www.google.com/"],
      "css": ["/css/App.css"],
      "js": ["/js/app.js", "/js/menu.js"],
      "run_at": "document_end"
    }
  ],

  "background": {
    "scripts": [/*"socket.io.js",*/"js/eventPage.js", "js/auth.js", "js/oauthFlow.js"],
    "persistent": true
  },

  "permissions": [
    "identity",
    "tabs",
    "cookies",
    "<all_urls>",
    "storage"
  ]
}